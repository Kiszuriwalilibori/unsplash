{
    "compilerOptions": {
        "target": "es2015",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": false,
        "noEmit": true,
        "jsx": "react",
        "baseUrl": "src",
        "paths": {
            "types/*": ["types/*"],
            "HOCs/*": ["HOCs/*"],
            "contexts/*": ["contexts/*"],
            "js*": ["js/*"],
            "hooks/*": ["hooks/*"],
            "components/*": ["components/*"],
            "styles/*": ["styles/*"],
            "images/*": ["images/*"],
            "pages": ["pages/*"],
            "reduxware": ["reduxware/*"],
            "assets/*": ["assets/*"],
            "icons/*": ["assets/icons/*"]
        }
    },
    "include": ["src"]
}
